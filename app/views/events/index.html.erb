  <div class="columns">
    <div class="column">
      <div id="map"></div>
      <script>
      function initMap() {
        var uluru = {lat: 49.2821004, lng: -123.1082745};

        var map = new google.maps.Map(document.getElementById('map'), {
          zoom: 12,
          center: uluru
        });
        var marker = new google.maps.Marker({
          position: uluru,
          map: map
        });
      }
    </script>
    <script async defer
      src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBE1Boy-A_UgLPNoOY7kOuj-6Yju4cEJCE&callback=initMap">
    </script>
    </div>
  </div>

